<%= review.rating %>
<%= review.comment %>

<%= if user_signed_in? %>
    <%= if review.user_id == current_user.id %>
        <%= link_to "Edit", edit_book_review_path(review.book, review) %>
        <%= link_to "Delete", book_review_path(review.book, review), method: :delete, data: {"Sure?"} %>
    <% end %>
<% end %>